[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C = BO
ST = La Paz
L = La Paz
O = SISFIPO
OU = Desarrollo
CN = 192.168.1.4

[v3_req]
basicConstraints = CA:FALSE
keyUsage = digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = *.localhost
IP.1 = 127.0.0.1
IP.2 = ::1
IP.3 = 192.168.1.4
IP.4 = 192.168.1.4
IP.5 = 192.168.15.245
